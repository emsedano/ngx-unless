{
  "name": "nopalws",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-unless",
    "publish": "sh publish-lib.sh",
    "clear:dist": "rm -rf dist",
    "copy-files": "cp README.md dist/ngx-unless & cp LICENSE dist/ngx-unless",
    "lib:version": "npm run clear:dist & sh bumpversion.sh",
    "bump:patch": "npm run lib:version -- patch",
    "bump:minor": "npm run lib:version -- minor",
    "bump:major": "npm run lib:version -- major",
    "build:patch": "npm run bump:patch & npm run build & npm run copy-files",
    "build:minor": "npm run bump:minor & npm run build & npm run copy-files",
    "build:major": "npm run bump:major & npm run build & npm run copy-files",
    "publish:patch": "npm run build:patch & npm run publish",
    "publish:minor": "npm run build:minor & npm run publish",
    "publish:major": "npm run build:major & npm run publish",
    "test": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0-rc.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "generate-changelog": "^1.8.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.1.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
